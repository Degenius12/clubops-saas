{
  "version": 2,
  "name": "clubops-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_API_URL": "https://clubops-backend.vercel.app",
    "VITE_APP_NAME": "ClubOps"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://clubops-backend.vercel.app"
    }
  }
}