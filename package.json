{
  "name": "clubops-saas",
  "version": "1.0.0",
  "description": "Premium gentlemen's club management SaaS platform",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "deploy": "npm run build && npm run deploy:frontend && npm run deploy:backend",
    "deploy:frontend": "cd frontend && npm run deploy",
    "deploy:backend": "cd backend && npm run deploy",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "playwright": "^1.57.0"
  },
  "keywords": [
    "saas",
    "club-management",
    "gentlemens-club",
    "dancer-management",
    "dj-queue",
    "premium-ui"
  ],
  "author": "ClubOps Development Team",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/clubops-saas.git"
  }
}
